{
	"name": "coblocks",
	"title": "CoBlocks",
	"version": "1.9.4",
	"private": false,
	"author": "Rich Tabor & Jeffrey Carandang from CoBlocks",
	"author_shop": "CoBlocks",
	"author_uri": "hello@coblocks.com",
