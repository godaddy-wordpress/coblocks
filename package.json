		"simple-git": "^3.16.0"
	},
	"lint-staged": {
		"*.scss": [
			"stylelint src/**/*.scss --fix"
		],
		"*.js": [
			"wp-scripts test-unit-js --config .dev/tests/jest/jest.config.js --silent --findRelatedTests --passWithNoTests",
			"yarn lint:js"
		],
		"*.php": [
			"yarn lint:php"
		]
	}
}
