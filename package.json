		"jest-fetch-mock": "^3.0.3",
		"jest-junit": "13.0.0",
		"jest-snapshot": "^27.4.5",
		"jsdom": "^16.7.0",
		"lint-staged": "^12.1.1",
		"lodash": "^4.17.21",
		"matchdep": "^2.0.0",
		"memize": "^1.0.5",
		"pa11y": "^6.2.3",
		"po2json": "^1.0.0-alpha",
		"prettier": "^2.5.1",
		"prop-types": "^15.8.1",
		"puppeteer": "^13.5.2",
		"react-masonry-component": "^6.2.1",
		"react-spring": "^9.4.4",
		"rtlcss-webpack-plugin": "^4.0.6",
		"simple-git": "^3.16.0",
		"tiny-swiper": "^2.2.0",
