{
	"name": "coblocks/gallery-collage",
	"category": "media",
	"attributes": {
		"images": {
			"type": "array",
			"default": [],
			"source": "query",
			"selector": ".wp-block-coblocks-gallery-collage__item",
			"query": {
				"url": {
					"source": "attribute",
					"selector": "img",
					"attribute": "src"
				},
				"link": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-link"
				},
				"imgLink": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-imglink"
				},
				"alt": {
					"source": "attribute",
					"selector": "img",
					"attribute": "alt",
					"default": ""
				},
				"id": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-id"
				},
				"index": {
					"source": "attribute",
					"selector": "img",
					"attribute": "data-index"
				},
				"caption": {
					"type": "string",
					"source": "children",
					"selector": "figcaption"
				}
			}
		},
		"shadow": {
			"type": "string",
			"default": "none"
		},
		"align": {
			"type": "string",
			"default": "wide"
		}
	},
	"title": "Collage",
	"textdomain": "coblocks",
	"editorScript": "blocks-3",
	"description": "Assemble images into a beautiful collage gallery."
}
