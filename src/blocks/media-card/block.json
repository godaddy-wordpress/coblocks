{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "coblocks/media-card",
	"category": "layout",
	"attributes": {
		"mediaPosition": {
			"type": "string",
			"default": "left"
		},
		"mediaAlt": {
			"type": "string",
			"source": "attribute",
			"selector": "figure img",
			"attribute": "alt",
			"default": ""
		},
		"mediaId": {
			"type": "number"
		},
		"mediaUrl": {
			"type": "string"
		},
		"mediaType": {
			"type": "string"
		},
		"mediaWidth": {
			"type": "number",
			"default": 55
		},
		"align": {
			"type": "string",
			"default": "wide"
		},
		"maxWidth": {
			"type": "number"
		},
		"hasImgShadow": {
			"type": "boolean",
			"default": false
		},
		"hasCardShadow": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"anchor": true,
		"align": [ "wide", "full" ],
		"html": false,
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"textdomain": "coblocks",
	"title": "Media Card"
}
