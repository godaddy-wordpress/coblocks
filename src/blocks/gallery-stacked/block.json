{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "coblocks/gallery-stacked",
	"category": "media",
	"attributes": {
		"captionStyle": {
			"type": "string"
		},
		"fullwidth": {
			"type": "boolean",
			"default": true
		},
		"gutter": {
			"type": "number",
			"default": 0
		},
		"gutterMobile": {
			"type": "number",
			"default": 0
		}
	},
	"supports": {
		"align": true,
		"anchor": true,
		"html": false,
		"color": {},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontStyle": true,
			"__experimentalFontWeight": true,
			"__experimentalLetterSpacing": true,
			"__experimentalTextTransform": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		}
	},
	"title": "Stacked"
}
